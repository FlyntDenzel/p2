generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
    id              String @id @default(cuid())
    name            String
    email           String
    skillsOffered   String[]  
    skillsWanted    String[]
    createdAt       DateTime @default(now())
}